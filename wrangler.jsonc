{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "renfo-weatherkit-proxy",
  "main": "cloudflare/weatherkit-proxy-worker.js",
  "compatibility_date": "2026-02-25",
  "workers_dev": false,
  "routes": [
    {
      "pattern": "www.renfo.app/api/weather*",
      "zone_name": "renfo.app"
    }
  ],
  "vars": {
    "ALLOWED_ORIGINS": "https://www.renfo.app,http://127.0.0.1:5500,http://localhost:5500",
    "WEATHERKIT_TOKEN_TTL_SECONDS": "1800"
  }
}
